<p style="color: green"><%= notice %></p>

<% content_for :title, "Usuarios" %>

<h1>Usuarios</h1>

<div id="usuarios">
  <% @usuarios.each do |usuario| %>
    <%= render usuario %>
    <p>
      <%= link_to "Mostrar este usuário", usuario %>
    </p>
  <% end %>
</div>

<%= link_to "Novo usuário", new_usuario_path %>
